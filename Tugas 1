#include <iostream>
using namespace std;

int main() 
{
    
    int bb,tb,ideal;
 
    cout<<"Masukkan Berat Badan (kg) : ";
    cin>>bb;
    cout<<"Masukkan Tinggi Badan (cm) : ";
    cin>>tb;
    ideal=bb / (tb * tb);
if (bb>=50 && tb>=160){
    cout << "Tubuh kamu ideal";
}
else
{
    cout << "Tubuh Kamu terlalu kurus";
}
        
return 0;
 
}
