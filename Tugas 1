    cout<<"Masukan Berat Badan Anda (kg) = ";
    cin>>bb;

    if(hasil<=150 && hasil>=-150)
{
    cout<<"Tubuh kamu Ideal"<<endl;
}
    else

    cout<<"Tubuh kamu terlalu kurus"<<endl;


